# Server Configuration
PORT=10000
NODE_ENV=production

# Database Configuration
DATABASE_URL=your_mongodb_url_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGINS=http://localhost:19006,http://localhost:19000,http://localhost:19001,exp://localhost:19000

# API Configuration
API_VERSION=v1
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Note: For development, make sure to use these Expo default ports
# Expo web: http://localhost:19006
# Expo dev client: http://localhost:19000
# Expo dev tools: http://localhost:19001
# Expo local: exp://localhost:19000 